<h1 class="offset1">Listing projects <small><%= page_entries_info @projects %></small></h1>

<% @projects.each do |project| %>
	<ul class="offset1">
		<li class = "text-info"><%= link_to project.title, project %></li>
		<li class = "no-bullet"><%= project.teaser %></li>
	</ul>
<% end %>

<%= paginate @projects %>


